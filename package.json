{
  "name": "sol-tcr",
  "version": "0.0.1",
  "description": "A generic, string-keyed TCR in Solidity",
  "scripts": {
    "install": "truffle install",
    "compile": "truffle compile",
    "coverage": "solidity-coverage",
    "deploy-rinkeby": "truffle migrate --network rinkeby",
    "deploy-mainnet": "truffle migrate --network mainnet",
    "test:truffle": "run-with-testrpc -l 20000000 'truffle test --network testing'",
    "test": "truffle test",
    "migrate": "truffle migrate --reset",
    "fix": "eslint --fix",
    "lint": "eslint",
    "ganache": "scripts/runGanache.sh"
  },
  "author": "Irene Lin <irene.lin@consensys.net>, Mira Zeitlin <miranda.zeitlin@consensys.net>, Yorke Rhodes <yorke.rhodesiv@consensys.net>",
  "license": "ISC",
  "dependencies": {
    "@gnosis.pm/dx-contracts": "^0.9.0",
    "@gnosis.pm/gnosis-core-contracts": "^1.0.4",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bignumber.js": "5.0.0",
    "bip39": "2.5.0",
    "bn.js": "4.11.8",
    "ethereumjs-abi": "0.6.5",
    "ethereumjs-wallet": "0.6.0",
    "ethjs": "0.3.1",
    "ethjs-provider-http": "0.1.6",
    "ethjs-query": "0.3.2",
    "ethjs-rpc": "0.1.8",
    "ethjs-util": "0.1.4",
    "lk-test-helpers": "0.1.3",
    "truffle": "4.1.13",
    "truffle-hdwallet-provider": "0.0.3"
  },
  "devDependencies": {
    "eslint": "4.15.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "run-with-testrpc": "^0.3.0",
    "solidity-coverage": "^0.4.15"
  }
}
